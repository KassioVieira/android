<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:paddingBottom="16dp"
    android:paddingLeft="16dp"
    android:paddingRight="16dp"
    android:background="#dafada"
    android:paddingTop="16dp">

    <TextView
        android:layout_width="match_parent"
        android:layout_height="130dp"
        android:layout_margin="5dp"
        android:background="@drawable/logo"
        android:gravity="center"
        android:textSize="12pt"
        android:textStyle="bold" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="3dp"
        android:background="#028109" />

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#dafada">

        <TextView
            android:id="@+id/catastraalimentacao"
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:layout_alignStart="@+id/Menu"
            android:layout_below="@+id/linearLayout"
            android:layout_marginStart="12dp"
            android:layout_marginTop="49dp"
            android:background="@drawable/dishes"
            android:clickable="true"
            android:gravity="center_horizontal"
            android:onClick="Telas" />

        <TextView
            android:id="@+id/textView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_below="@+id/catastraalimentacao"
            android:layout_marginStart="10dp"
            android:layout_marginTop="21dp"
            android:text="@string/addalimento"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/inforelatorio"
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:layout_alignBaseline="@+id/catastraalimentacao"
            android:layout_alignBottom="@+id/catastraalimentacao"
            android:layout_alignStart="@+id/textView11"
            android:layout_marginRight="35dp"
            android:layout_marginStart="28dp"
            android:background="@drawable/questionmark"
            android:clickable="true"
            android:onClick="Telas" />

        <TextView

            android:id="@+id/textView11"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/textView3"
            android:layout_alignBottom="@+id/textView3"
            android:layout_alignParentEnd="true"
            android:layout_marginEnd="16dp"
            android:text="@string/infomacaoalimentas"
            android:textStyle="bold" />

        <LinearLayout
            android:id="@+id/linearLayout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:orientation="horizontal">

        </LinearLayout>

        <Button
            android:id="@+id/Volta"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignEnd="@+id/textView11"
            android:layout_alignParentBottom="true"
            android:layout_alignStart="@+id/textView3"
            android:layout_marginBottom="21dp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:layout_weight="1"
            android:background="@drawable/botao_lindo3"
            android:onClick="Telas"
            android:text="Voltar"
            android:textColor="#ffffff" />

    </RelativeLayout>


</LinearLayout>